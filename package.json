{
  "name": "fundevogel",
  "version": "1.0.0",
  "description": "Source code of fundevogel.de",
  "author": "S1SYPHOS <hello@twobrain.io>",
  "homepage": "https://fundevogel.de",
  "main": "Gulpfile.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/two-brain/fundevogel.de.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/two-brain/fundevogel.de/issues"
  },
  "keywords": [
    "gulp",
    "kirby",
    "cms",
    "starterkit"
  ],
  "scripts": {
    "start": "gulp",
    "build": "NODE_ENV=production gulp build",
    "assets": "gulp styles && gulp scripts && gulp images && gulp fonts",
    "styles": "gulp styles",
    "scripts": "gulp scripts",
    "images": "gulp images",
    "fonts": "gulp fonts",
    "deploy": "NODE_ENV=production gulp build && gulp deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@barba/core": "2.9.7",
    "animejs": "3.2.0",
    "baguettebox.js": "1.11.1",
    "dropkickjs": "^2.2.4",
    "infinite-scroll": "3.0.6",
    "layzr.js": "2.2.2",
    "macy": "2.5.1",
    "svg4everybody": "2.1.9",
    "tailwindcss": "1.4.6",
    "tiny-slider": "2.9.2",
    "tippy.js": "6.2.3"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "autoprefixer": "9.8.0",
    "browser-sync": "2.26.7",
    "child_process": "1.0.2",
    "del": "5.1.0",
    "eslint": "7.0.0",
    "eslint-config-google": "0.14.0",
    "glyphhanger": "3.2.0",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "7.0.1",
    "gulp-babel": "8.0.0",
    "gulp-clean-css": "4.3.0",
    "gulp-connect-php": "1.0.3",
    "gulp-eslint": "6.0.0",
    "gulp-favicons": "2.4.0",
    "gulp-filter": "6.0.0",
    "gulp-flatten": "0.4.0",
    "gulp-if": "3.0.0",
    "gulp-imagemin": "7.1.0",
    "gulp-newer": "1.4.0",
    "gulp-postcss": "8.0.0",
    "gulp-rename": "2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-stylelint": "13.0.0",
    "gulp-svgstore": "7.0.1",
    "gulp-terser": "1.2.0",
    "gulp-uglify": "3.0.2",
    "imagemin-pngquant": "8.0.0",
    "precss": "4.0.0",
    "stylelint": "13.5.0",
    "stylelint-config-sass-guidelines": "7.0.0",
    "vinyl-named": "1.1.0",
    "webpack-stream": "5.2.1"
  },
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines",
    "rules": {}
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "amd": true,
      "commonjs": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "at-rule-no-unknown": [
        "off"
      ],
      "indent": [
        "error",
        4
      ],
      "max-len": [
        "warn",
        {
          "ignoreComments": true
        }
      ],
      "one-var": [
        "off"
      ],
      "require-jsdoc": [
        "off"
      ]
    }
  },
  "browserslist": [
    "last 3 versions",
    "> 1%",
    "IE >= 9"
  ]
}
